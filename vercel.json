{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api-docs", "dest": "/api-docs" },
    { "src": "/swagger.json", "dest": "/swagger.json" },
    { "src": "/api/step1", "dest": "/api/step1.js" },
    { "src": "/api/verify-otp", "dest": "/api/verify-otp.js" }
  ]
}
